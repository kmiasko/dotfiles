URxvt*termName        : rxvt-256color
URxvt*loginShell      : true
URxvt.perl-ext-common : default,matcher,clipboard
URxvt.url-launcher    : /usr/bin/chromium
URxvt.matcher.button  : 1
URxvt.scrollBar       : false

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
!Xft.rgba: none 
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

*background: #002b36
*foreground: #657b83
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3


URxvt.depth:                32
URxvt.geometry:             160x40
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0

URxvt*shading: 15

URxvt*saveLines: 12000

URxvt*foreground: #657b83
URxvt*background: #002b36

URxvt*font: xft:Fura Mono for Powerline Plus Nerd File Types Mono Plus Octicons Plus Pomicons:style=Regular:antialias=true:hinting=true:pixelsize=15
URxvt*boldFont: xft:Fura Mono for Powerline Plus Nerd File Types Mono Plus Octicons Plus Pomicons:style=Regular:antialias=true:hinting=true:bold:pixelsize=15
!URxvt*font: xft:PragmataPro:antialias=true:hinting=true:pixelsize=14
!URxvt*boldFont: xft:PragmataPro:antialias=true:hinting=true:bold:pixelsize=14
!URxvt*font: xft:Inconsolata\-g for Powerline:style=Regular:antialias=true:hinting=true:pixelsize=16
!URxvt*boldFont: xft:Inconsolata\-g for Powerline:style=Bold:antialias=true:hinting=true:bold:pixelsize=16
!URxvt*font: xft:Fira Mono:style=Regular:antialias=true:hinting=true:pixelsize=14
!URxvt*boldFont: xft:Fira Mono:style=Bold:antialias=true:hinting=true:bold:pixelsize=14

URxvt*scrollBar: false 
URxvt*scrollBar_right: false
URxvt*scrollstyle: plain
