! Just testing
URxvt*termName        : rxvt-256color
URxvt*loginShell      : true
URxvt.perl-ext-common : default,matcher,clipboard
URxvt.url-launcher    : /usr/bin/chromium
URxvt.matcher.button  : 1
URxvt.scrollBar       : false

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
!Xft.rgba: none 
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt.depth:                32
URxvt.geometry:             160x40
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0

URxvt*shading: 15

URxvt*saveLines: 12000


URxvt*font: xft:Fura Mono for Powerline Plus Nerd File Types Mono Plus Octicons Plus Pomicons:style=Regular:antialias=true:hinting=true:pixelsize=15
URxvt*boldFont: xft:Fura Mono for Powerline Plus Nerd File Types Mono Plus Octicons Plus Pomicons:style=Regular:antialias=true:hinting=true:bold:pixelsize=15
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollstyle: plain

*VT100.Translations: #override \
              <Key>BackSpace: string(0x7F)\n\
              <Key>Delete:    string("\033[3~")\n\
              <Key>Home:      string("\033[1~")\n\
              <Key>End:       string("\033[4~")
*ttyModes: erase ^?

